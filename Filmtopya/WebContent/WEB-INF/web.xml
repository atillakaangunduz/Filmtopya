<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Filmtopya</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>filmaddServlet</servlet-name>
    <servlet-class>com.Servlet.addfilmServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>filmaddServlet</servlet-name>
    <url-pattern>/addfilm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.Servlet.registerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logindemo</servlet-name>
    <servlet-class>com.Servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logindemo</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>filmlit</servlet-name>
    <servlet-class>com.Servlet.viewmovie</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logindemo</servlet-name>
    <url-pattern>/viewmovie</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>filmekle</servlet-name>
    <servlet-class>com.Servlet.addfilmadminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>filmekle</servlet-name>
    <url-pattern>/addfilmadmin</url-pattern>
  </servlet-mapping>
</web-app>